# brew utils have precedence over macOS utils
add_to_path top /opt/homebrew/bin

# Use brew GNU utils (gsed, gcut, gwc, ...) without the g prefix
add_to_path top /opt/homebrew/opt/coreutils/libexec/gnubin

# Path to software installed with pip3 install --user
add_to_path top ~/Library/Python/3.9/bin
add_to_path top ~/Library/Python/3.10/bin

# txt2regex: use brew gettext binaries
add_to_path bottom /usr/local/Cellar/gettext/0.19.8.1/bin/
